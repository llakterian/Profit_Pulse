{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-undef": "error",
        "react-hooks/exhaustive-deps": "warn",
        "import/no-unresolved": "error"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ],
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        }
    }
}